{
  "name": "trendberry-admin",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src src-example private webpack.config.js",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "src-example",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^(components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    }
  },
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "jest-cli": "^21.0.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-assets-by-type-plugin": "^0.1.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-server-source-map": "^0.2.0",
    "webpack-blocks-split-vendor": "^0.2.1",
    "webpack-child-config-plugin": "^0.1.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-spawn-plugin": "^0.1.2"
  },
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "append-query": "^2.0.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "delay": "^2.0.0",
    "dom-helpers": "^3.2.1",
    "draft-js": "^0.10.1",
    "draft-js-export-html": "^1.1.0",
    "draft-js-import-html": "^1.1.0",
    "draftjs-utils": "^0.8.6",
    "enzyme": "^2.9.1",
    "express": "^4.15.4",
    "express-device": "^0.4.2",
    "express-force-ssl": "^0.3.2",
    "history": "^4.7.2",
    "immutable": "^3.x.x",
    "isomorphic-fetch": "^2.2.1",
    "jss": "8.1.0",
    "jss-preset-default": "^3.0.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.10",
    "material-ui-icons": "^1.0.0-beta.5",
    "normalizr": "^3.2.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-jss": "^7.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-transition-group": "^1.2.0",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0",
    "redux-form-submit": "^0.1.3",
    "redux-mock-store": "^1.2.3",
    "redux-saga": "^0.15.6",
    "redux-saga-thunk": "^0.5.2",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.3",
    "simple-load-script": "^1.0.2",
    "source-map-support": "^0.4.17",
    "universal-cookie": "^2.1.0",
    "validator": "^8.1.0"
  }
}
